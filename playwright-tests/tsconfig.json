{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "moduleResolution": "node",
    "lib": [ "ES2022" ],
    "paths": {
      "@tests/*": [
        "./*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@support/*": [
        "support/*"
      ],
      "@root/*": [
        "../*"
      ],
      "@helpers/*": [
        "helpers/*"
      ],
      "@components/*": [
        "components/*"
      ],
    }
  }
}
